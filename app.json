{
  "expo": {
    "name": "Ciao QR",
    "slug": "ciao-qr",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.6",
    "orientation": "portrait",
    "primaryColor": "#FFEA2A",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFEA2A"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "ch.feuerschutz.ciao.qr",
      "buildNumber": "1.0.6",
      "supportsTablet": true,
      "associatedDomains": [
        "applinks:ciao.feuerschutz.ch"
      ]
    },
    "android": {
      "package": "ch.feuerschutz.ciao.qr",
      "versionCode": 7,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "ciao.feuerschutz.ch",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": []
    },
    "description": ""
  }
}
