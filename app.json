{
  "expo": {
    "name": "Ciao QR",
    "slug": "ciao-qr",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFEA2A"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "ch.feuerschutz.ciao.qr",
      "buildNumber": "1.0.2",
      "supportsTablet": true,
      "associatedDomains": ["applinks:ciao.feuerschutz.ch"]
    },
    "android": {
      "package": "ch.feuerschutz.ciao.qr",
      "versionCode": 3,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "ciao.feuerschutz.ch",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "permissions": []
    },
    "description": ""
  }
}
